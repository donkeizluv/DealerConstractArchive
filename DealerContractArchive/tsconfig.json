{
  "compileOnSave": false,
  "compilerOptions": {
    "noImplicitAny": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "declaration": false,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "./wwwroot",
    "wwwroot",
    "lib",
    "types",
    "typings",
    "typing",
    "**/*.ts"
  ]
}
